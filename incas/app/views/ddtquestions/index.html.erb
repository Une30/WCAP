<%= render "layouts/navbar"%> 
<div class="wrap">

<h1>لیست سوالات ddtquestion :</h1>
<hr><br>
<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Value1</th>
      <th>Value2</th>
      <th>Value3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ddtquestions.each do |ddtquestion| %>
      <tr>
        <td><%= ddtquestion.question_type %></td>
        <td><%= ddtquestion.value1 %></td>
        <td><%= ddtquestion.value2 %></td>
        <td><%= ddtquestion.value3 %></td>
        <td><%= link_to 'نمایش', ddtquestion %></td>
        <td><%= link_to 'ویرایش', edit_ddtquestion_path(ddtquestion) %></td>
        <td><%= link_to 'حذف', ddtquestion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ddtquestion', new_ddtquestion_path %>
</div>
<h1>Listing barts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @barts.each do |bart| %>
      <tr>
        <td><%= bart.task_id %></td>
        <td><%= link_to 'Show', bart %></td>
        <td><%= link_to 'Edit', edit_bart_path(bart) %></td>
        <td><%= link_to 'Destroy', bart, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bart', new_bart_path %>
